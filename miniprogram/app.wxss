/**app.wxss**/
/* 隐藏滚动条 */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: #ffffff;
}

/* 全局顶部栏css  linui组件 */
.capsule-bar-bg {
  width:100%;
  position:fixed;
  top:0;
  left:0;
  opacity: 0.5;
}




 /**基础风格样式**/
@import './components/towxml/style/main.wxss';
 
 
/**如果页面有动态主题切换，则需要将使用到的样式全部引入**/
 
/**主题配色（浅色样式）**/
@import './components/towxml/style/theme/light.wxss';
 
/**主题配色（深色样式）**/
@import './components/towxml/style/theme/dark.wxss';